
# === Required for live hotel prices via Amadeus (Self-Service APIs) ===
AMADEUS_API_KEY=your_amadeus_api_key
AMADEUS_API_SECRET=your_amadeus_api_secret
# Set ENV to 'test' for Amadeus test environment; 'prod' for production
AMADEUS_ENV=test

# === Google Places API (for restaurants) ===
GOOGLE_PLACES_API_KEY=your_google_places_api_key

# Optional: enable MOCK mode (uses sample data without calling external APIs)
MOCK=0
